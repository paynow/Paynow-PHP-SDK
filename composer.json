{
  "name": "paynow/php-sdk",
  "description": "PHP SDK for the Paynow API",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Paynow\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "require": {
    "php": ">=5.6.3",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.1",
    "victorjonsson/markdowndocs": "^1.3"
  }, 
    "minimum-stability": "dev",
    "prefer-stable": true
}
